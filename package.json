{
  "name": "@dfrankland/node-project-config",
  "version": "1.0.0",
  "description": "My personal, reusable Node.js project configurations",
  "scripts": {
    "dev": "webpack serve --hot --open",
    "build": "webpack",
    "lint": "eslint --ext .js --ext .ts --ext .tsx .",
    "typecheck": "npm run typecheck:source && npm run typecheck:tests && npm run typecheck:cypress",
    "typecheck:source": "tsc -p ./tsconfig.base.json --noEmit",
    "typecheck:tests": "tsc -p ./tsconfig.tests.json --noEmit",
    "typecheck:cypress": "tsc -p ./cypress/tsconfig.json --noEmit",
    "format:write": "prettier --write '**/*.js' '**/*.ts' '**/*.tsx'",
    "format:check": "prettier --check '**/*.js' '**/*.ts' '**/*.tsx'",
    "commitlint": "commitlint",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest",
    "test:integration": "start-server-and-test dev http-get://localhost:8080 cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dfrankland/node-project-config.git"
  },
  "author": "dfrankland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dfrankland/node-project-config/issues"
  },
  "homepage": "https://github.com/dfrankland/node-project-config#readme",
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@bahmutov/cypress-extends": "^1.1.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@cypress/code-coverage": "^3.9.11",
    "@cypress/webpack-preprocessor": "^5.9.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "acorn": "^8.5.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.5.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jest-junit": "^12.3.0",
    "jest-styled-components": "^7.0.5",
    "react-refresh": "^0.10.0",
    "webpack": "^5.56.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.0",
    "@commitlint/types": "^13.2.0",
    "@jest/types": "^27.2.4",
    "@testing-library/react": "^12.1.2",
    "@types/babel__core": "^7.1.16",
    "@types/jest": "^27.0.2",
    "@types/prettier": "^2.4.1",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.26",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.14",
    "core-js": "^3.18.1",
    "cypress": "^8.5.0",
    "eslint": "^7.32.0",
    "jest": "^27.2.4",
    "prettier": "^2.4.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "start-server-and-test": "^1.14.0",
    "styled-components": "^5.3.1",
    "typescript": "^4.4.3",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.1"
  }
}
